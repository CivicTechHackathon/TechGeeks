                <th>To</th>
                <th>Property</th>
                <th>View</th>
              </tr>
            </thead>
            <tbody>

              <script type="text/javascript">
                for(i = 0;i<trans.length;i++) {
                  if( trans[i].Record["status"] == "0" ) {
                    ctr();
                    document.write('<tr>');

                    document.write('<td><strong><center>'+count+'</center></strong></td>');
                    document.write('<td>'+trans[i].Key+'</td>');
                    document.write('<td>'+trans[i].Record["cnic"]+'</td>');
                    document.write('<td>'+trans[i].Record["transfercnic"]+'</td>');
                    document.write('<td>'+trans[i].Record["property"]+'</td>');


                    document.write('<td> <form action="/opentrans" method="POST"> <input type="hidden" name="tid" value="'+trans[i].Key+'"> <input type="submit" class="btn btn-success" value="View"> </form> </td>');


                    document.write('</tr>');

                  }
                }
              </script>


